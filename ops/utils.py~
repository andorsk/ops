from optparse import OptionParser
import argparse





# Main Function Calls
FUNCTION_MAP = {
    'img2s3' : img2s3
}

#Global option set with overriding options by functioin
ap = argparse.ArgumentParser(description='S3 Client Parser')
ap.add_argument('command', choices=FUNCTION_MAP.keys())
ap.add_argument('-v', '--verbosity', help="Increase vebose")
ap.add_argument('-f', '--file', help="input file")
args = ap.parse_args()

def img2s3():
    print(ap.f)
    



run = FUNCTION_MAP[args.command]
run()



